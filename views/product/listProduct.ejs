<!DOCTYPE html>
<html lang="en">

    <%- include('../partials/head')%>

<body>
    <%- include('../partials/header')%>
    <main>

        <div class="container">
            <div class="titular">
                <h2 class="titulo">Productos.</h2>
            </div>
            <section class="detalle">
                <button>New Product</button>
                <div class="productos">
                    <% products.forEach(product => { %>
                        <%- include('./product', { product })%>
                    <% })%>
                </div>
            </section>

        </div>
    </main>
    
    <%- include('../partials/footer')%>

</body>

</html>